{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "school": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/school",
      "sourceRoot": "projects/school/src",
      "prefix": "my",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/school",
            "index": "projects/school/src/index.html",
            "main": "projects/school/src/main.ts",
            "polyfills": "projects/school/src/polyfills.ts",
            "tsConfig": "projects/school/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/school/src/favicon.ico",
              "projects/school/src/assets",
              "projects/school/src/manifest.webmanifest"
            ],
            "allowedCommonJsDependencies": [
              "firebase",
              "firebase/app",
              "lodash",
              "lodash/dropRight",
              "@angular/common/locales/fr",
              "@firebase/app",
              "@firebase/database",
              "@firebase/firestore",
              "@angular/fire/firestore",
              "@firebase/functions",
              "@firebase/performance",
              "@firebase/remote-config"
            ],
            "styles": [
              "projects/school/src/styles.scss",
              "projects/school/src/scss/theme.scss",
              "node_modules/reveal.js/plugin/highlight/monokai.css",
              "node_modules/reveal.js/dist/reveal.css"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js",
              "node_modules/reveal.js/dist/reveal.js",
              "node_modules/reveal.js/plugin/highlight/highlight.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/school/src/environments/environment.ts",
                  "with": "projects/school/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "projects/school/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "school:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "school:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "school:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/school/src/test.ts",
            "polyfills": "projects/school/src/polyfills.ts",
            "tsConfig": "projects/school/tsconfig.spec.json",
            "karmaConfig": "projects/school/karma.conf.js",
            "assets": [
              "projects/school/src/favicon.ico",
              "projects/school/src/assets",
              "projects/school/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/school/src/styles.scss"
            ],
            "scripts": [
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/school/tsconfig.app.json",
              "projects/school/tsconfig.spec.json",
              "projects/school/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/school/e2e/protractor.conf.js",
            "devServerTarget": "school:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "school:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    }
  },
  "cli": {
    "analytics": "ad6aac95-3931-4302-aa75-09eeea260793"
  },
  "defaultProject": "school"
}
